{
  "hosting": {
    "public": "totallynotthefrontendtrust",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "function": "myFunction"
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}
